{
	"name": "plugin/owc-gravityforms-openagenda",
	"description": "GravityForms integration for OpenAgenda",
	"type": "wordpress-plugin",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"require": {
		"cmdisp/monolog-microsoft-teams": "^1.3",
		"php": "^7.4|^8.0",
		"php-di/php-di": "^6.0"
	},
	"require-dev": {
		"10up/wp_mock": "~0.5",
		"friendsofphp/php-cs-fixer": "^2.18",
		"mockery/mockery": "^1.0",
		"phpunit/phpunit": "^9.0",
		"szepeviktor/phpstan-wordpress": "^1.3.0"
	},
	"autoload": {
		"psr-4": {
			"OWC\\OpenAgenda\\": "./src/OpenAgenda"
		}
	},
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": true,
		"sort-packages": true
	},
	"authors": [
		{
			"name": "Yard | Digital Agency",
			"email": "owc@yard.nl",
			"homepage": "https://www.yard.nl"
		}
	]
}
